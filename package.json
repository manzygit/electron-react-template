{
  "name": "project_name",
  "version": "1.0.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "npx vite .",
    "build": "npx vite build",
    "preview": "npm run build && electron .",
    "package": "npm run build && npx electron-builder"
  },
  "author": "manzygit",
  "license": "ISC",
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.12",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^39.2.0",
    "electron-builder": "^26.0.12",
    "sass": "^1.94.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-electron": "^0.29.0"
  },
  "build": {
    "appId": "com.your-app-name",
    "productName": "Your App Name",
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "directories": {
      "output": "release",
      "buildResources": "assets"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "assets/icon.ico"
    }
  }
}
